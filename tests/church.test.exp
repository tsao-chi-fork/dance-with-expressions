zero(f, x) { x }
one(f, x) { f(x) }
two(f, x) { f(f(x)) }
three(f, x) { f(f(f(x))) }
four(f, x) { f(f(f(f(x)))) }
five(f, x) { f(f(f(f(f(x))))) }

add1(n) {
  (f, x) {
    f(n(f, x))
  }
}

add(m, n) {
  (f, x) {
    m(f, n(f, x))
  }
}

mul(m, n) {
  (f, x) {
    m(n(f), x)
  }
}

pow(m, n) {
  n(m)
}

id(x) { x }

zero

one
add1(one)

two
three
